<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<gradient android:startColor="#bb000000" android:endColor="#cc000000"
				android:angle="270" />
			<stroke android:width="1dp" android:color="#66000000" />
			<corners android:radius="8dip" />
			<padding android:left="10dip" android:top="8dip"
				android:right="10dip" android:bottom="8dip" />
		</shape>
	</item>
	<item>
		<shape>
			<gradient android:startColor="#aa000000" android:endColor="#bb000000"
				android:angle="270" />
			<stroke android:width="1dp" android:color="#66111111" />
			<corners android:radius="8dip" />
			<padding android:left="10dip" android:top="8dip"
				android:right="10dip" android:bottom="8dip" />
		</shape>
	</item>
</selector>